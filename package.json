{
  "name": "ying-yang-react",
  "version": "1.0.0",
  "description": "React boilerplate with WebPack 2, Babel, Redux, Ramda, HMR, Enzyme, Selenium ...",
  "scripts": {
    "start": "webpack-dashboard -c blue -- node __env__/webpack.dev.js",
    "build": "rm -rf dist && ./node_modules/.bin/webpack --config __env__/webpack.prod.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "e2e": "web-driver",
    "suite": "npm run test && npm run e2e",
    "publish": "npm run suite && npm run build && gh-pages -d dist",
    "updates": "node ./node_modules/npm-check-updates/bin/ncu -u && npm update"
  },
  "author": "Meillet Robin <robin.meillet@epitech.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kMeillet/ying-yang-react.git"
  },
  "bugs": {
    "url": "https://github.com/kMeillet/ying-yang-react/issues"
  },
  "homepage": "https://github.com/kMeillet/ying-yang-react/#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.4.1",
    "axios": "^0.14.0",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.25.0",
    "css-mqpacker": "^5.0.1",
    "csscomb-loader": "0.0.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "flowtype-loader": "^0.2.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^15.1.1",
    "npm-check-updates": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "progress-bar-webpack-plugin": "^1.9.0",
