    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.22",
    "webpack-browser-plugin": "^1.0.12",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "core-decorators": "^0.12.3",
    "ramda": "^0.22.1",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-redux": "^4.4.0",
    "react-router": "^4.0.0-2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
